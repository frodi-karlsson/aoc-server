{
  "name": "aocserver",
  "version": "1.0.0",
  "description": "Simple web server for fetching, testing and submitting Advent of Code solutions",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/frodi-karlsson/aoc-server.git"
  },
  "author": "frodi-karlsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frodi-karlsson/aoc-server/issues"
  },
  "homepage": "https://github.com/frodi-karlsson/aoc-server#readme",
  "dependencies": {
    "axios": "^1.5.0",
    "chromedriver": "^116.0.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "selenium-webdriver": "^4.12.0"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/selenium-webdriver": "^4.1.16",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
