{
  "name": "aoc-server-demo",
  "version": "1.0.0",
  "description": "Typescript dev example that uses the aoc-server for Advent of Code interactions",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "author": "frodi-karlsson",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
